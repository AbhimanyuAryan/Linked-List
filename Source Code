int main()
{
    head =NULL;

    int n, i, x;

    printf("How many Numbers?: \n");
    scanf("%d", &n);

    for(i= 0; i<n; i++)
    {
        printf("Enter the number: \n");
        scanf("%d",&x);
        Insert(x);
        Print();
    }
    return 0;
