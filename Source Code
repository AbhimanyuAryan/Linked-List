#include <stdio.h>
#include <stdlib.h>

struct Node{
int data;
struct Node *next;
};
struct Node* head;

void Insert(int x)
{
    struct Node* temp = (Node*)malloc(sizeof(struct Node));
    temp->data;
    temp->next;

    temp->next = head;
    head= temp;
}

void Print()
{
    struct Node * temp= head;
    print("List is: ");
    while(temp != NULL)
    {
        printf("%d", temp->data);
        temp= temp->next;
    }
    print("\n");
}

